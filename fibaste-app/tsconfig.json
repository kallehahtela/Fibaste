{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@views/*": [
        "app/views/*"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@utils/*": [
        "app/utils/*"
      ],
      "@ui/*": [
        "app/ui/*"
      ],
      "@store/*": [
        "app/store/*"
      ],
      "@icons/*": [
        "app/icons/*"
      ],
      "@api/*": [
        "app/api/*"
      ],
      "@navigator/*": [
        "app/navigator/*"
      ],
    }
  }
}